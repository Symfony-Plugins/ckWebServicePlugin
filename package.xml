<?xml version="1.0" encoding="UTF-8"?>
<package packagerversion="1.4.6" version="2.0"
	xmlns="http://pear.php.net/dtd/package-2.0"
	xmlns:tasks="http://pear.php.net/dtd/tasks-1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0 http://pear.php.net/dtd/tasks-1.0.xsd http://pear.php.net/dtd/package-2.0 http://pear.php.net/dtd/package-2.0.xsd">
	<name>ckWebServicePlugin</name>
	<channel>plugins.symfony-project.org</channel>
	<summary>WebService API Plugin.</summary>
	<description>
		This plugin exposes your actions as a webservice api.
	</description>
	<lead>
		<name>Christian Kerl</name>
		<user>chrisk</user>
		<email>christian-kerl@web.de</email>
		<active>yes</active>
	</lead>
	<date>2008-08-19</date>
	<version>
		<release>2.1.0</release>
		<api>2.1.0</api>
	</version>
	<stability>
		<release>stable</release>
		<api>stable</api>
	</stability>
	<license uri="http://www.opensource.org/licenses/mit-license.php">
		MIT license
	</license>
	<notes>-</notes>
	<contents>
		<dir name="/">
			<file role="data" name="README" />
			<file role="data" name="LICENSE" />
			<dir name="config">
				<file role="data" name="config.php" />
			</dir>
			<dir name="lib">
				<dir name="controller">
					<file role="data"
						name="ckWebServiceController.class.php" />
				</dir>
				<dir name="task">
					<file role="data"
						name="ckWebServiceGenerateWsdlTask.class.php" />
				</dir>
				<dir name="util">
					<file role="data" name="ckString.class.php" />
					<file role="data"
						name="ckComponentEventListener.class.php" />
				</dir>
                <dir name="vendor/ckWsdlGenerator">
                    <file role="data" name="ckDocBlockParser.class.php" />
                    <file role="data" name="ckDOMSerializable.class.php" />
                    <file role="data" name="ckWsdlGenerator.class.php" />
                    <dir name="wsdl">
                        <file role="data" name="ckWsdlBindingDecorator.class.php" />
                        <file role="data" name="ckWsdlDefinitions.class.php" />
                        <file role="data" name="ckWsdlHttpBindingDecorator.class.php" />
                        <file role="data" name="ckWsdlMessage.class.php" />
                        <file role="data" name="ckWsdlOperation.class.php" />
                        <file role="data" name="ckWsdlPart.class.php" />
                        <file role="data" name="ckWsdlPortDecorator.class.php" />
                        <file role="data" name="ckWsdlPortType.class.php" />
                        <file role="data" name="ckWsdlService.class.php" />
                        <file role="data" name="ckWsdlSoapBindingDecorator.class.php" />
                        <file role="data" name="ckWsdlSoapPortDecorator.class.php" />
                    </dir>
                    <dir name="xsd">
                        <file role="data" name="ckXsdArrayType.class.php" />
                        <file role="data" name="ckXsdComplexType.class.php" />
                        <file role="data" name="ckXsdComplexTypeElement.class.php" />
                        <file role="data" name="ckXsdNamespace.class.php" />
                        <file role="data" name="ckXsdSimpleType.class.php" />
                        <file role="data" name="ckXsdType.class.php" />
                    </dir>
                </dir>
				<file role="data" name="ckSoapHandler.class.php" />
				<file role="data"
					name="ckSoapParameterFilter.class.php" />
			</dir>
		</dir>
	</contents>
	<dependencies>
		<required>
			<php>
				<min>5.1.0</min>
			</php>
			<pearinstaller>
				<min>1.4.1</min>
			</pearinstaller>
			<package>
				<name>symfony</name>
				<channel>pear.symfony-project.com</channel>
				<min>1.1.0</min>
				<max>1.2.0</max>
				<exclude>1.2.0</exclude>
			</package>
		</required>
	</dependencies>
	<phprelease />
	<changelog />
</package>